{
  "name": "Gradesafe",
  "version": "1.0.0",
  "description": "Gradebook implemented for uta secure programming class",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf dist/* && cross-env NODE_ENV=development webpack-dev-server --hot",
    "start2": "cd example && webpack-dev-server --inline --content-base dist --history-api-fallback --port 8083 --mode development",
    "build": "babel -d dist/ src/",
    "build:watch": "babel -w -d dist/ src/",
    "build:docs": "rm -rf docs/*.js  && rm -rf docs/*.css && cd example && NODE_ENV=production webpack --mode production",
    "cm": "git-cz",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wldarden/Gradesafe.git"
  },
  "keywords": [
    "react",
    "package",
    "template"
  ],
  "author": "Will Darden",
  "license": "MIT",
